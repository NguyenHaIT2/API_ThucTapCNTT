<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.GroupRoleEntity" table="group_role" schema="school">
        <composite-id mapped="true" class="entity.GroupRoleEntityPK">
            <key-property name="groupid">
                <column name="groupid" sql-type="int"/>
            </key-property>
            <key-property name="roleid">
                <column name="roleid" sql-type="int"/>
            </key-property>
        </composite-id>
        <many-to-one name="groupByGroupid" class="entity.GroupEntity">
            <column name="groupid"/>
        </many-to-one>
        <many-to-one name="roleByRoleid" class="entity.RoleEntity">
            <column name="roleid"/>
        </many-to-one>
    </class>
</hibernate-mapping>